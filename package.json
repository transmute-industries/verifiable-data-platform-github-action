  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^1.2.6",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
